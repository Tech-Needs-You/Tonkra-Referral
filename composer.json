{
    "name": "tonkra/referral",
    "description": "Referral system package for Tonkra SMS",
    "type": "package",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Tonkra\\Referral\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tonkra\\Referral\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tonkra\\Referral\\ReferralServiceProvider"
            ],
            "aliases": {
                "Referral": "Tonkra\\Referral\\Facades\\Referral"
            }
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "SIRJAW",
            "email": "sirjaw4u@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}